/*---------------------------------------------------*/
*{
  font-family: "Open Sans", Sans-Serif;
}
.margin-auto {
  margin: auto !important;
}
/*---------------------------------------------------Back ground colours*/
.full-bg{
  background-color: #f5ce42;
}
/*---------------------------------------------------list*/
.width-keypoints {
  width: 95%;
}


/*---------------------------------------------------full infomation list*/
.ul-styles{
  list-style-type: none;
  padding: 5px;
}

.full-info-row {
  border-radius: 12px;
}

.bg-full-info {
  background-color: black !important;
  border-radius: 12px;
  margin: 10px auto !important;
}

/*---------------------------------------------------images after list*/
.img-mid{
  width: 70%;
  margin-left: 5px;
  margin-right: 5px;
  border-radius: 50px;
}
/*---------------------------------------------------footer*/
footer {
  background-color: #f5ce42;
}
.rbb-logo{
  width: 40%;
  mix-blend-mode: normal;
}
.rbb-logo:hover{
  background-color: Green;
  border-radius: 12px;
}
